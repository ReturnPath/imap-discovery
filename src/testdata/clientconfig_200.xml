<clientConfig version="1.1">
  <emailProvider id="unoeuro.com">
    <domain>bisgaard.cc</domain>
    <incomingServer type="imap">
      <hostname>mail.unoeuro.com</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>mail.unoeuro.com</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
      <outgoingServer type="smtp">
      <hostname>asmtp.unoeuro.com</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
    </outgoingServer>
  </emailProvider>
  <webMail>
    <loginPage url="https://webmail.unoeuro.com/"/>
  </webMail>
</clientConfig>
